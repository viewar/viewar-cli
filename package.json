{
  "name": "viewar-cli",
  "version": "0.11.1",
  "description": "ViewAR SDK Command Line Interface",
  "main": "index.js",
  "scripts": {
    "bundle": "rollup -c",
    "build": "npx babel src --out-dir dist/",
    "clean": "rimraf dist",
    "start": "npm run build; node ./dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "version": "git add -A",
    "postversion": "git push origin HEAD && git push --tags",
    "prepublish": "npm run bundle",
    "format": "prettier --single-quote --trailing-comma es5 '**/*.{js,md,json}' --write"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/viewar/viewar-cli.git"
  },
  "keywords": [],
  "author": "Karl Hofer <kh@viewar.com>",
  "license": "MIT",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,jsx}": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "bin": {
    "viewar-cli": "dist/index.js",
    "viewar": "dist/index.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/polyfill": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@robmarr/rollup-plugin-shebang": "^1.0.1",
    "babel-loader": "^8.0.5",
    "babel-preset-minify": "^0.5.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "prettier": "^1.17.0",
    "rimraf": "^2.6.3",
    "rollup": "^1.10.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-babel-minify": "^8.0.0",
    "rollup-plugin-commonjs": "^9.3.4",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-preserve-shebang": "^0.1.6"
  },
  "dependencies": {
    "archiver": "^3.0.0",
    "chalk": "^2.4.2",
    "check-node-version": "^3.3.0",
    "commander": "^2.20.0",
    "easy-table": "^1.1.1",
    "inquirer": "^6.3.1",
    "latest-version": "^5.1.0",
    "os": "^0.1.1",
    "prompt": "^1.0.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "semver": "^6.0.0",
    "shelljs": "^0.8.3"
  }
}
