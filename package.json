{
  "name": "viewar-cli",
  "version": "0.5.0",
  "description": "ViewAR SDK Command Line Interface",
  "main": "index.js",
  "contributors": [
    "Karl Hofer <kh@viewar.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/viewar/viewar-cli.git"
  },
  "license": "MIT",
  "scripts": {
    "build": "npm link",
    "version": "git add -A",
    "postversion": "git push origin HEAD && git push --tags",
    "format": "prettier --single-quote --trailing-comma es5 '**/*.{js,md,json}' --write"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,md,jsx}": [
      "prettier --single-quote --trailing-comma es5 --write",
      "git add"
    ]
  },
  "engines": {
    "node": ">=8"
  },
  "bin": {
    "viewar-cli": "index.js",
    "viewar": "index.js"
  },
  "dependencies": {
    "ajv": "^6.5.0",
    "archiver": "^2.1.1",
    "chalk": "^2.3.0",
    "commander": "^2.13.0",
    "easy-table": "^1.1.1",
    "inquirer": "^5.1.0",
    "latest-version": "^4.0.0",
    "prompt": "^1.0.0",
    "request": "^2.87.0",
    "request-promise": "^4.2.2",
    "semver": "^5.6.0",
    "shelljs": "^0.8.1"
  },
  "devDependencies": {
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "^1.15.2"
  }
}
